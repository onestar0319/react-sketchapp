{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "ReactComponent": true,
    "React$Component": true,
    "ReactClass": true,
    "log": true,
    "React$Element": true,
    "COSAlertWindow": true,
    "NSTextField": true,
    "NSString": true,
    "NSFont": true,
    "NSTextView": true,
    "NSView": true,
    "MSRect": true,
    "NSMakeRect": true,
    "MSColor": true,
    "MSShapeGroup": true,
    "MSArtboardGroup": true,
    "MSOvalShape": true,
    "MSTextLayer": true,
    "MSLayerGroup": true,
    "MSRectangleShape": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "no-duplicate-imports": [0]
  }
}
