{
  "name": "react-sketchapp",
  "version": "0.0.0",
  "description": "A React renderer for Sketch.app",
  "main": "lib/index.js",
  "license": "MIT",
  "author": "Jon Gold <jon.gold@airbnb.com> (http://jon.gold)",
  "contributors": [
    "Ben Wilkins <ben.wilkins@airbnb.com>"
  ],
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build:plugin": "webpack -c",
    "build:react": "babel src -d lib",
    "build": "npm run build:react && npm run build:plugin",
    "clean": "rimraf lib react-example.sketchplugin",
    "check": "npm run flow && npm run lint",
    "flow": "flow",
    "lint": "eslint .",
    "prepublish": "npm run clean && npm run check && npm run build:react",
    "test": "test test/test.js",
    "watch:plugin": "webpack --watch",
    "watch:react": "babel src -d lib --watch",
    "watch": "npm run watch:react & npm run watch:plugin"
  },
  "dependencies": {
    "fbjs": "^0.8.5",
    "invariant": "^2.2.2",
    "ramda": "^0.22.1",
    "react": "~15.3.2"
  },
  "keywords": [
    "sketch",
    "sketchapp",
    "react",
    "reactjs",
    "renderer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jongold/react-sketchapp"
  },
  "bugs": {
    "url": "https://github.com/jongold/react-sketchapp/issues"
  },
  "homepage": "https://github.com/jongold/react-sketchapp",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "jest": "^17.0.3",
    "json-loader": "^0.5.4",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.3"
  }
}
