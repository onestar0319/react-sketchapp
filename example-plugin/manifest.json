{
  "name": "react-example",
  "description": "Example react-sketchapp plugin",
  "author": "Jon Gold",
  "homepage": "http://github.com/jongold/react-sketchapp",
  "version": 0.1,
  "identifier": "com.jongold.react-sketchapp",
  "updateURL": "https://github.com/downloads/jongold/react-sketchapp/react-sketchapp.json",
  "compatibleVersion": 1,
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Example: Profiles",
      "identifier": "profiles",
      "shortcut": "ctrl shift r",
      "script": "Profiles.js"
    },
    {
      "name": "Example: Styleguide",
      "identifier": "styleguide",
      "shortcut": "ctrl shift s",
      "script": "Styleguide.js"
    }
  ],
  "menu": {
    "items": [
      "profiles",
      "styleguide"
    ]
  }
}
